{
  "name": "awssat/discord-notification-channel",
  "description": "Discrod Notification Channel for laravel.",
  "keywords": ["laravel", "notifications", "discrod"],
  "license": "MIT",
  "authors": [
    {
      "name": "Bader Almutairi",
      "email": "phpfalcon@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "^6.0",
    "illuminate/notifications": "~5.8.0|^6.0|^7.0",
    "laravel/slack-notification-channel": "^2.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Awssat\\Notifications\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awssat\\Notifications\\DiscordChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
